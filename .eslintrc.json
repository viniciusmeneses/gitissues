{
  "parser": "babel-eslint",
  "extends": ["airbnb", "plugin:react-native/all"],
  "env": {
    "jest": true,
    "browser": true
  },
  "plugins": ["react", "jsx-a11y", "import"],
  "rules": {
    "react/jsx-filename-extension": [
      "error",
      {
        "extensions": [".js", ".jsx"]
      }
    ],
    "import/prefer-default-export": "off"
  },
  "globals": {
    "__DEV__": true
  }
}
